import random

matriz = [[0 for _ in range(3)] for _ in range(3)]
suma = 0
for i in range(3):
    for j in range(3):
        matriz[i][j] = random.randint(0, 10)
        suma += matriz[i][j]

diferencia = suma - 21
matriz[2][2] -= diferencia
print("Matriz 3x3 con n√∫meros aleatorios ajustados:")
for fila in matriz:
    print(fila)

suma_final = 0
for fila in matriz:
    suma_final += sum(fila)

print("Suma de los elementos:", suma_final)
